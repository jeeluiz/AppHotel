    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Home"
    BackgroundColor="#ff556b2f">


    <ContentPage.Content>
        <Frame
            Margin="20"
            BackgroundColor="#F5F5DC"
            BorderColor="DarkGreen"
            VerticalOptions="CenterAndExpand">
            <Grid
                x:Name="G3"
                ColumnDefinitions="*,*,*,*"
                ColumnSpacing="1"
                RowDefinitions="*,*,*,0.7*,2*,*"
                RowSpacing="1"
                
                />
        </Frame>
    </ContentPage.Content>


</ContentPage>